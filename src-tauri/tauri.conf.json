{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../out",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "maaagic"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.ico",
        "icons/128x128.ico",
        "icons/256x256.io",
        "icons/icon.icns"
      ],
      "identifier": "dev.maaagic.ui",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "nsis": {
          "installerIcon": "./icons/128x128.ico"
        }
      }
    },
    "security": {
      "csp": null,
      "dangerousRemoteDomainIpcAccess": [
        {
          "scheme": "https",
          "domain": "tauri.localhost",
          "windows": [
            "main"
          ],
          "plugins": [
            "app",
            "shell",
            "os",
            "event",
            "window"
          ]
        }
      ]
    },
    "windows": [
      {
        "fullscreen": false,
        "title": "maaagic",
        "center": true,
        "resizable": true,
        "decorations": false,
        "transparent": true,
        "height": 920,
        "width": 1300,
        "minHeight": 600,
        "minWidth": 800

      }
    ]
  }
}
